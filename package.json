  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "steno": "^4.0.2",
    "stonex": "^0.1.3",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ]
}
